Algoritmo distanciasdedospuntos
	definir xpunto1,ypunto1,xpunto2,ypunto2,xpunto3,ypunto3,hipotenusa,s como real;
	Escribir "digite la x punto 1";
	leer xpunto1;
	Escribir "digite la y punto 1";
	leer ypunto1;
	Escribir "digite la x punto 2";
	leer xpunto2;
	Escribir "digite la y punto 2";
	leer ypunto2;
	Escribir "digite la x punto 3";
	leer xpunto3;
	escribir "digite la y punto 3";
	leer ypunto3;
	distancia1=raiz((xpunto2-xpunto1)^2+(ypunto2-ypunto1)^2)
	escribir "la distancia 1 es =",distancia1;
	distancia2=raiz((xpunto3-xpunto2)^2+(ypunto3-ypunto2)^2)
	escribir "la distancia 2 es =",distancia2;
	distancia3=raiz((xpunto1-xpunto3)^2+(ypunto1-ypunto3)^2)
	escribir "la distancia 3 es =",distancia3;
	s=(distancia2+distancia3+distancia1)/2
	escribir"el semiperimetro es =",s;
	area=raiz(s*(s-distancia1)*(s-distancia2)*(s-distancia3))
	escribir "area es =",area;
FinAlgoritmo
