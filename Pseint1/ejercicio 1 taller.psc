Algoritmo montodescuento
	definir monto como real;
	escribir "digite el monto que desea aplicar"
	leer monto
	si monto<50000 Entonces
		escribir" no hay descuento, debes de pagar",monto;
	SiNo
		si monto>100000 entonces
			escribir" aplica el descuento de 10%, el total a pagar seria ",monto*0.9;
		sino 
			escribir" aplica al descuento de 5%, el total a pagar seria ", monto*0.95;
		FinSi
		
	FinSi
	
FinAlgoritmo
